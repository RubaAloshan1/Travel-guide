 <script>
  // Get the star element
  const starElement1 = document.getElementById('starRating1');

  // Check if the rating is already stored in local storage
  if (!localStorage.getItem('rating1')) {
    // Set default rating to 0
    localStorage.setItem('rating1', '0');
  }

  // Retrieve the stored rating
  starElement1.textContent = localStorage.getItem('rating1');

  // Add click event listener to the star element
  starElement1.addEventListener('click', function() {
    // Get the current rating
    let currentRating = parseInt(starElement1.textContent);

    // Check if the user has already liked the rating
    if (localStorage.getItem('liked1')) {
      // User has already liked, so remove the like
      localStorage.removeItem('liked1');
      // Decrease the rating by 1
      currentRating--;
    } else {
      // User hasn't liked yet, so add the like
      localStorage.setItem('liked1', 'true');
      // Increase the rating by 1
      currentRating++;
    }

    // Update the rating text
    starElement1.textContent = currentRating;

    // Save the updated rating to local storage
    localStorage.setItem('rating1', currentRating);
  });
</script>


